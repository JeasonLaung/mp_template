@import '../style/init.wxss';
@import '../../icon.wxss';
.m-search {
	padding: 16rpx 30rpx;
	display: block;
	background-color: #ffffff;
	box-shadow: 0px 0px 15rpx 1rpx 
		rgba(6, 0, 1, 0.05);
}


.m-search-fixed{
	left: 0;
	right: 0;
	top: 0;
	position: fixed;
	z-index: 66;
}
input.m-search_input{
	margin-top: 13rpx;
}
.m-search_input {
	border-radius: rpx;
	height: 65rpx!important;
	border: solid 1rpx #eee;
	padding: 0 20rpx 0 30rpx;
	display: flex;
	align-items: center;
	font-weight: normal!important;
	justify-content: space-between;
	font-size: 24rpx;
}



.m-search_input_placeholder {
	line-height: 56rpx;
	color: #cccccc;
}

.icon-search{
	color: #cccccc;
}
